
<div class="span12">
  <div class="span17 last">
    <section class="ac-container">
       <legend>Manual de Usuario </legend>
      <%i=0%>
      <% @link.each do |help| %>
      <div>
        <input id="<%= "ac-#{i}" %>" name="accordion-1" type="radio" checked />
        <label for="<%= "ac-#{i}" %>" ><%=help  %></label>
        <article class="ac-large">
           <%= image_tag "help/help#{i+1}.png", :width => '670x600' %>
          <p><%= @link_helps[i] %></p>
        </article>
      </div>
      <%i+=1%>
      <% end %>
    </section>
  </div>

</div>